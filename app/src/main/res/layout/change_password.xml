<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="fill_parent"
	android:layout_height="match_parent"
	android:background="@color/colorPrimary"
	android:fillViewport="true">
	
	
	
	
<RelativeLayout
	android:layout_width="match_parent"
	android:visibility="visible"
	android:layout_height="match_parent">
	
	
	<RelativeLayout
		android:layout_height="match_parent"
		android:layout_width="wrap_content">
		
		<RelativeLayout
			android:id="@+id/my_toolbar_layout"
			android:layout_width="match_parent"
			android:layout_height="@dimen/_55sdp"
			android:background="@drawable/submit_background"
			android:orientation="vertical">
			
			<TextView
				android:id="@+id/tvBillsHeader"
				style="@style/text_roboto_600wt_24sp_black"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerVertical="true"
				android:layout_gravity="center_vertical"
				android:layout_marginStart="@dimen/margin"
				android:text="@string/change_password_title" />
		</RelativeLayout>
		

		<RelativeLayout
			android:id="@+id/rlTextField"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:layout_marginStart="@dimen/_10sdp"
			android:layout_marginEnd="@dimen/_10sdp"
			android:layout_marginBottom="@dimen/_10sdp"
			android:layout_below="@+id/my_toolbar_layout"
			android:layout_marginTop="@dimen/_25sdp">
			
			<TextView
				android:id="@+id/tvLogin"
				style="@style/text_header_600wt_20sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_alignParentStart="true"
				android:text="@string/form" />
			
			<RelativeLayout
				android:id="@+id/rlPassword"
				android:layout_below="@+id/tvLogin"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_18sdp"
				android:layout_width="wrap_content">
			
			<TextView
				android:id="@+id/tvPassword"
				style="@style/text_header_600wt_20sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
			
				android:layout_centerVertical="true"
				android:layout_marginEnd="@dimen/_8sdp"
				android:text="@string/password" />
			
			<TextView
				android:id="@+id/tvAsterisk"
				android:layout_centerHorizontal="true"
				android:layout_width="@dimen/_10sdp"
				android:layout_height="@dimen/_10sdp"
				android:textColor="@color/colorRed"
				android:layout_centerVertical="true"
				android:layout_toEndOf="@+id/tvPassword"
				android:text="@string/asterisk" />
				
			</RelativeLayout>
			
			
			
			<EditText
				android:id="@+id/et_new_password"
				style="@style/text_400wt_17sp_nintyfivepercentage_opacity"
				android:layout_width="match_parent"
				android:layout_height="@dimen/editTextViewHeight"
				android:paddingStart="@dimen/_10sdp"
				android:layout_below="@+id/rlPassword"
				android:layout_marginTop="@dimen/_14sdp"
				android:background="@drawable/username_background"
				android:hint="@string/password_hint"
				android:inputType="textPersonName"
				android:maxLength="45"
				android:paddingLeft="@dimen/itemPadding" />
			
			<RelativeLayout
				android:id="@+id/rlConfirmPassword"
				android:layout_below="@+id/et_new_password"
				android:layout_marginTop="@dimen/_18sdp"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content">
				
				<TextView
					android:id="@+id/tv_Cpassword"
					style="@style/text_header_600wt_20sp"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					
					android:layout_centerVertical="true"
					
					android:layout_marginEnd="@dimen/_8sdp"
					android:text="@string/password" />
				
				<TextView
					android:id="@+id/tvConfirmAsterisk"
					android:layout_centerHorizontal="true"
					android:layout_width="@dimen/_10sdp"
					android:layout_height="@dimen/_10sdp"
					android:textColor="@color/colorRed"
					android:layout_centerVertical="true"
					android:layout_toEndOf="@+id/tv_Cpassword"
					android:text="@string/asterisk" />
			
			</RelativeLayout>

			
			<com.google.android.material.textfield.TextInputLayout
				android:id="@+id/etTextInput"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:layout_below="@+id/rlConfirmPassword"
				android:layout_marginTop="@dimen/_12sdp"
				app:hintEnabled="false"
				app:passwordToggleEnabled="false">
				
				
				<EditText
					android:id="@+id/et_rePassword"
					style="@style/text_roboto_400wt_16sp_shadesGray"
					android:layout_width="match_parent"
					android:layout_height="@dimen/editTextViewHeight"
					android:paddingStart="@dimen/_10sdp"
					android:background="@drawable/password_background"
					android:hint="@string/reenter_password_hint"
					android:inputType="textPassword"
					android:paddingLeft="@dimen/itemPadding" />
			</com.google.android.material.textfield.TextInputLayout>
			
			<RelativeLayout
				android:id="@+id/rlOldPasswordTop"
				android:layout_height="wrap_content"
				android:layout_marginTop="@dimen/_10sdp"
				android:layout_below="@+id/etTextInput"
				android:layout_width="wrap_content">
				
				<RelativeLayout
					android:id="@+id/rlOldPassword"
					android:layout_height="wrap_content"
					android:layout_width="wrap_content">
					
					<TextView
						android:id="@+id/tv_OldPassword"
						style="@style/text_header_600wt_20sp"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_centerVertical="true"
						android:layout_marginTop="@dimen/_18sdp"
						android:layout_marginEnd="@dimen/_8sdp"
						android:text="@string/old_password" />
					
					<TextView
						android:id="@+id/tvOldPasswordAsterisk"
						android:layout_centerHorizontal="true"
						android:layout_width="@dimen/_10sdp"
						android:layout_height="@dimen/_10sdp"
						android:textColor="@color/colorRed"
						android:layout_centerVertical="true"
						android:layout_toEndOf="@+id/tv_OldPassword"
						android:text="@string/asterisk" />
				
				</RelativeLayout>
				
				<com.google.android.material.textfield.TextInputLayout
					android:id="@+id/etOldPasswordInput"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/rlOldPassword"
					android:layout_marginTop="@dimen/_2sdp"
					app:hintEnabled="false"
					app:passwordToggleEnabled="false">
					
					
					<EditText
						android:id="@+id/et_OldPassword"
						style="@style/text_roboto_400wt_16sp_shadesGray"
						android:layout_width="match_parent"
						android:layout_height="@dimen/editTextViewHeight"
						android:paddingStart="@dimen/_10sdp"
						android:background="@drawable/password_background"
						android:hint="@string/old_password_hint"
						android:inputType="textPassword"
						android:paddingLeft="@dimen/itemPadding" />
				</com.google.android.material.textfield.TextInputLayout>

				
			</RelativeLayout>
			

			
			<RelativeLayout
				android:id="@+id/rlSubmit"
				android:layout_height="wrap_content"
				android:layout_below="@+id/rlOldPasswordTop"
				android:layout_marginTop="@dimen/_25sdp"
				android:background="@drawable/submit_btn_background"
				android:layout_width="match_parent">
			
			<TextView
				android:id="@+id/btn_login"
				style="@style/text_roboto_700wt_20sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_centerHorizontal="true"
				android:layout_centerVertical="true"
				android:layout_marginTop="@dimen/_30sdp"
			
				android:text="@string/submit" />
				
			</RelativeLayout>
			
			
			<RelativeLayout
				android:id="@+id/rlDiscard"
				android:layout_height="wrap_content"
				android:layout_below="@+id/rlSubmit"
				android:layout_marginTop="@dimen/_20sdp"
				android:background="@drawable/discard_btn_background"
				android:layout_width="match_parent">
				
				<TextView
					android:id="@+id/btn_discard"
					style="@style/text_roboto_700wt_20sp_light_color_black"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_centerHorizontal="true"
					android:layout_centerVertical="true"
					android:layout_marginTop="@dimen/_30sdp"
					android:text="@string/discard" />
			
			</RelativeLayout>
			

			
		</RelativeLayout>
	
	</RelativeLayout>

</RelativeLayout>
</androidx.core.widget.NestedScrollView>