				android:layout_width="match_parent"
				android:layout_height="match_parent" />
			
			
			
			<include
				android:id="@+id/graph"
				layout="@layout/bar_graph"
				android:layout_below="@+id/schemeGraph"
				android:layout_width="match_parent"
				android:layout_height="match_parent" />
			
				


			
			
			
			
