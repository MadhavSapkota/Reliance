<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/colorPrimary"
	android:fillViewport="true">
	
	<RelativeLayout
		android:layout_width="match_parent"
		android:layout_height="wrap_content">
		
		<RelativeLayout
			android:id="@+id/my_toolbar_layout"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:background="@drawable/dashboard_retailer_shape"
			android:orientation="vertical">
			
			<TextView
				android:id="@+id/tv_retailer_dashboard_header_hello"
				style="@style/text_roboto_600wt_28sp"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="@dimen/_10sdp"
				android:layout_marginTop="@dimen/_40sdp"
				android:text="@string/retailer_dashboard_heading" />
			
			
			<TextView
				android:id="@+id/tv_retailer_dashboard_header"
				style="@style/text_roboto_600wt_28sp"
				android:layout_below="@+id/tv_retailer_dashboard_header_hello"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_marginStart="@dimen/_10sdp"
				android:text="@string/retailer_dashboard_heading_retailer_name" />
			
			
			<RelativeLayout
				android:id="@+id/rl_retailer_transcation"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				android:layout_below="@+id/tv_retailer_dashboard_header"
				android:layout_marginStart="@dimen/_10sdp"
				android:layout_marginEnd="@dimen/_10sdp"
				android:layout_marginBottom="@dimen/_10sdp"
				android:layout_marginTop="@dimen/_13sdp"
				>
				
				
				<androidx.cardview.widget.CardView
					android:id="@+id/card_view_total_transcations"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					app:cardCornerRadius="@dimen/_7sdp">
					
					<RelativeLayout
						android:id="@+id/rl_total_transactions"
						android:layout_width="match_parent"
						android:layout_height="@dimen/_90sdp"
						android:background="@color/lightWhiteGray">
						
						<ImageView
							android:id="@+id/imageView_transcation_icon"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_marginStart="@dimen/_15sdp"
							android:layout_marginEnd="@dimen/_15sdp"
							android:src="@drawable/retailer_transcation_icon" />
						
						
						<RelativeLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							
							android:layout_marginEnd="@dimen/_5sdp"
							android:layout_marginBottom="@dimen/_14sdp"
							android:layout_toEndOf="@+id/imageView_transcation_icon">
							
							<TextView
								android:id="@+id/tv_bill_number"
								style="@style/text_roboto_14sp_400_opacity60"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginBottom="@dimen/_1sdp"
								android:text="@string/total_transaction" />
							
							<RelativeLayout
								android:id="@+id/rl_retailer_transaction"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_below="@+id/tv_bill_number"
								android:layout_centerVertical="true">
								
								
								<TextView
									android:id="@+id/tv_transcation_amount"
									style="@style/text_roboto_28sp_700_black"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginBottom="@dimen/_4sdp"
									android:text="@string/total_transaction_amount" />
								
								<TextView
									android:id="@+id/tv_bills_amount"
									style="@style/text_roboto_11sp_400_opacity60percentage"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_below="@+id/tv_transcation_amount"
									android:text="@string/total_transcation_pre_vat" />
							</RelativeLayout>
						</RelativeLayout>
					
					</RelativeLayout>
				
				
				</androidx.cardview.widget.CardView>
				
				<androidx.cardview.widget.CardView
					android:id="@+id/card_view_total_scheme"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_below="@+id/card_view_total_transcations"
					android:layout_marginTop="@dimen/_15sdp"
					android:layout_marginBottom="@dimen/_15sdp"
					app:cardCornerRadius="@dimen/_7sdp">
					
					<RelativeLayout
						android:id="@+id/rl_total_scheme"
						android:layout_width="match_parent"
						android:layout_height="@dimen/_90sdp"
						android:background="@color/lightWhiteGray">
						
						<ImageView
							android:id="@+id/imageView_total_scheme"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_marginStart="@dimen/_15sdp"
							android:layout_marginEnd="@dimen/_15sdp"
							android:src="@drawable/retailer_scheme_icon" />
						
						
						<RelativeLayout
							android:layout_width="match_parent"
							android:layout_height="wrap_content"
							android:layout_centerVertical="true"
							android:layout_marginEnd="@dimen/_5sdp"
							android:layout_marginBottom="@dimen/_14sdp"
							android:layout_toEndOf="@+id/imageView_total_scheme">
							
							<TextView
								android:id="@+id/tv_total_scheme"
								style="@style/text_roboto_14sp_400_opacity60"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_marginBottom="@dimen/_1sdp"
								android:text="@string/total_scheme" />
							
							<RelativeLayout
								android:id="@+id/rl_retailer_total_scheme_amount"
								android:layout_width="wrap_content"
								android:layout_height="wrap_content"
								android:layout_below="@+id/tv_total_scheme"
								android:layout_centerVertical="true">
								
								<TextView
									android:id="@+id/tv_scheme_amount"
									style="@style/text_roboto_28sp_700_black"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginBottom="@dimen/_5sdp"
									android:text="@string/total_scheme_amount" />
							</RelativeLayout>
						</RelativeLayout>
					
					</RelativeLayout>
				</androidx.cardview.widget.CardView>
			</RelativeLayout>
		</RelativeLayout>
		
		<RelativeLayout
			android:id="@+id/dashboardRecyclerView"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginTop="@dimen/_24sdp"
			android:layout_marginStart="@dimen/_10sdp"
			android:layout_marginEnd="@dimen/_13sdp"
			android:layout_below="@id/my_toolbar_layout">
