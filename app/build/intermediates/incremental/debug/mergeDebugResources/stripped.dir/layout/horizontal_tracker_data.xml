<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content">
	
	<RelativeLayout
		android:id="@+id/rlHorizontalPercentage"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		
		<RelativeLayout
			android:id="@+id/ll_complete_status"
			android:layout_width="@dimen/_80sdp"
			android:layout_height="@dimen/_25sdp"
			android:layout_marginEnd="@dimen/_5sdp"
			android:background="@drawable/bills_all_background"
			android:gravity="center_horizontal"
			android:orientation="horizontal">
			
			<TextView
				android:id="@+id/txt_btn"
				android:layout_width="wrap_content"
				android:layout_height="wrap_content"
				style="@style/text_roboto_14sp_500_white"
				android:textAllCaps="false"
			     android:layout_centerVertical="true"
				android:text="@string/coil_mat" />
		</RelativeLayout>
	</RelativeLayout>

</RelativeLayout>