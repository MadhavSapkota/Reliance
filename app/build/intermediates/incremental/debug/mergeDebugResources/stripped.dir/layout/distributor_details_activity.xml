					<RelativeLayout
						android:id="@+id/rl_calioo"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:layout_marginTop="@dimen/_10sdp">
						
						<TextView
							android:id="@+id/tv_calioo"
							style="@style/text_roboto_400wt1_16sp_black"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginStart="@dimen/_5sdp"
							android:text="@string/distributor_total_amount" />
						
						<TextView
							android:id="@+id/tv_calio_price"
							style="@style/text_roboto_700wt_16sp_black"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_alignParentRight="true"
							android:layout_marginEnd="@dimen/_10sdp"
							android:text="@string/bill_details_calio_max_price" />
						
						
						<View
							android:id="@+id/view_linem"
							android:layout_width="match_parent"
							android:layout_height="@dimen/_1sdp"
							android:layout_below="@+id/tv_calio_price"
							android:layout_marginStart="@dimen/_5sdp"
							android:layout_marginTop="@dimen/_5sdp"
							android:layout_marginEnd="@dimen/_8sdp"
							android:background="@color/colorLineBar" />
					
					</RelativeLayout>
					
					
