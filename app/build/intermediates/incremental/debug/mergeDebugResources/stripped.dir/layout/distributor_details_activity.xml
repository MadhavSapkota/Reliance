							style="@style/text_roboto_400wt1_16sp_black"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_marginStart="@dimen/_5sdp"
							android:text="@string/distributor_current_scheme" />
						
						<TextView
							android:id="@+id/tv_current_scheme_price"
							style="@style/text_roboto_700wt_16sp_black"
							android:layout_width="wrap_content"
							android:layout_height="wrap_content"
							android:layout_alignParentRight="true"
							android:layout_marginEnd="@dimen/_10sdp"
							android:layout_marginBottom="@dimen/_10sdp"
							android:text="@string/bill_details_calio_price" />
					
					
					</RelativeLayout>
				
				
